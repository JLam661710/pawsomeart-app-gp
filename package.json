    "express-rate-limit": "^8.0.1",
    "form-data": "^4.0.4",
    "lucide-react": "^0.540.0",
    "multer": "^2.0.2",
    "multiparty": "^4.2.3",
    "node-fetch": "^2.7.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.47.2",
