# 飞书应用配置
# Feishu App Configuration
FEISHU_APP_ID=your_feishu_app_id_here
FEISHU_APP_SECRET=your_feishu_app_secret_here
FEISHU_APP_TOKEN=your_feishu_app_token_here
FEISHU_ORDERS_TABLE_ID=your_feishu_orders_table_id_here

# 飞书备份配置（可选）
# Feishu Backup Configuration (Optional)
FEISHU_BACKUP_APP_TOKEN=your_backup_app_token_here
FEISHU_BACKUP_ORDERS_TABLE_ID=your_backup_orders_table_id_here

# 飞书容量管理配置
# Feishu Capacity Management Configuration
FEISHU_CAPACITY_WARNING_THRESHOLD=16000
FEISHU_CAPACITY_MAX_THRESHOLD=18000

# 开发环境配置
# Development Environment Configuration
NODE_ENV=development

# 测试模式（设置为'1'或'true'启用飞书API模拟）
# Test Mode (Set to '1' or 'true' to enable Feishu API mocking)
MOCK_FEISHU=false

# Vercel部署标识（由Vercel自动设置，无需手动配置）
# Vercel Deployment Flag (Automatically set by Vercel, no manual configuration needed)
# VERCEL=1

# CI环境标识（由CI系统自动设置）
# CI Environment Flag (Automatically set by CI systems)
# CI=true