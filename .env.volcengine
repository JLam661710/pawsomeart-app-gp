# 火山引擎生产环境配置
# 注意：部署到火山引擎后，需要将此文件重命名为 .env.production

# 火山引擎API网关配置
# 实际的API网关地址
VITE_API_BASE_URL=https://sd2nfsos05ikp3tvhepug.apigateway-cn-shanghai.volceapi.com

# 环境标识
VITE_APP_ENV=volcengine
NODE_ENV=production

# 调试配置
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=error

# 功能开关
VITE_ENABLE_MOCK=false
VITE_ENABLE_ANALYTICS=true

# 应用信息
VITE_APP_NAME=PawsomeArt
VITE_APP_VERSION=2.0.0

# 火山引擎特定配置
VITE_VOLCENGINE_REGION=cn-north-1
VITE_VOLCENGINE_FUNCTION_TIMEOUT=300000

# 文件上传配置
VITE_MAX_FILE_SIZE=52428800
VITE_MAX_FILES=10
VITE_UPLOAD_TIMEOUT=300000

# 错误追踪（可选）
# VITE_SENTRY_DSN=your_sentry_dsn
# VITE_SENTRY_ENVIRONMENT=production